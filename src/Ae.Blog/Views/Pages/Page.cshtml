@model Ae.Blog.Models.PageModel
@{
    if (!Model.Title.Equals("Index", StringComparison.InvariantCultureIgnoreCase))
    {
        ViewData["Title"] = Model.Title;
    }

    switch (Model.Category)
    {
        case "raw":
            Layout = null;
            break;
    }
}
@Html.Raw(Model.Content)